{
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "docs",
        "recurse": true,
        "verbose": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "docdash": {
        "static": true,
        "sort": true,
        "search": true,
        "collapse": true,
        "typedefs": true,
        "removeQuotes": "none",
        "scripts": [],
        "menu": {
            "Repository": {
                "href": "https://github.com/DaxBot/node-canopen",
                "target": "_blank",
                "class": "menu-item",
                "id": "repository"
            }
        }
    },
    "templates": {
        "default": {
            "useLongnameInNav": true,
            "includeDate": false
        }
    }
}